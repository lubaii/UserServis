#!/bin/bash
# Скрипт для установки переменных окружения
# Использование: 
#   1. Скопируйте этот файл: cp SETUP_ENV.sh.example SETUP_ENV.sh
#   2. Отредактируйте SETUP_ENV.sh и укажите ваши реальные значения
#   3. Запустите: source SETUP_ENV.sh

export DB_URL=jdbc:postgresql://localhost:5432/usersdb
export DB_USERNAME=postgres
export DB_PASSWORD=your_password_here

echo "Переменные окружения установлены:"
echo "DB_URL=$DB_URL"
echo "DB_USERNAME=$DB_USERNAME"
echo "DB_PASSWORD=***"

